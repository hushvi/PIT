{

  "Routes": [
    {
      "DownstreamPathTemplate": "/api/NewsObjs",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44357
        },
        {
          "Host": "localhost",
          "Port": 44358
        }
      ],
      "UpstreamPathTemplate": "/gateway/news",
      "UpstreamHttpMethod": [ "GET" ],
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      }
    },

    {
      "DownstreamPathTemplate": "/api/NewsObjs/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44357
        }
      ],
      "UpstreamPathTemplate": "/gateway/news/{id}",
      "UpstreamHttpMethod": [ "GET" ],
    },


    {
      "DownstreamPathTemplate": "/api/TeamObjs/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44385
        }
      ],
      "UpstreamPathTemplate": "/gateway/teams/{id}",
      "UpstreamHttpMethod": [ "GET" ],
      "Key": "team"
    },
    {
      "DownstreamPathTemplate": "/api/teamGames/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44380
        }
      ],
      "UpstreamPathTemplate": "/gateway/teamGames/{id}",
      "UpstreamHttpMethod": [ "GET" ],
      "Key": "teamgames"
    }
  ],
  "Aggregates": [
    {
      "ReRouteKeys": [
        "team",
        "team-games"
      ],
      "UpstreamPathTemplate": "/api/team-games/{id}"
    }
  ]
}

